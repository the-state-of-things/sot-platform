{
    "name": "sot-platform",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "npm run build:client && npm run build:server",
        "lint": "eslint --fix **/*.{js,ts,vue}",
        "build:client": "vue-cli-service build",
        "build:server": "babel server --out-dir server-prod --copy-files --extensions \".ts\"",
        "client": "vue-cli-service serve",
        "server": "nodemon server/bin/www --exec babel-node --extensions \".js\",\".ts\" --presets \"@babel/preset-env\",\"@babel/preset-typescript\"",
        "start": "node server-prod/bin/www",
        "test:e2e": "vue-cli-service test:e2e",
        "test:unit": "vue-cli-service test:unit",
        "test": "npm run test:unit && npm run test:e2e",
        "prettier": "pretty-quick"
    },
    "dependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.8.4",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-typescript": "^7.8.3",
        "@mdi/font": "^4.9.95",
        "@mdi/js": "^4.9.95",
        "axios": "^0.18.1",
        "babel-core": "^7.0.0-bridge.0",
        "babel-loader": "^8.0.6",
        "babel-preset-env": "^1.7.0",
        "cookie-parser": "~1.4.3",
        "core-js": "^2.6.11",
        "cors": "^2.8.5",
        "db-migrate": "^0.11.6",
        "db-migrate-pg": "^0.5.1",
        "debug": "~2.6.9",
        "dotenv": "^8.2.0",
        "express": "~4.16.0",
        "express-subdomain": "^1.0.5",
        "morgan": "~1.9.0",
        "pg": "^7.18.1",
        "vue": "^2.6.11",
        "vue-class-component": "^7.2.2",
        "vue-js-modal": "^1.3.33",
        "vue-property-decorator": "^8.4.0",
        "vue-router": "^3.1.5",
        "vuedraggable": "^2.23.2",
        "vuetify": "^1.5.22"
    },
    "devDependencies": {
        "@babel/node": "^7.8.4",
        "@types/chai": "^4.2.8",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.2",
        "@types/mocha": "^5.2.7",
        "@types/morgan": "^1.7.37",
        "@types/node": "^12.12.26",
        "@typescript-eslint/eslint-plugin": "^1.13.0",
        "@typescript-eslint/parser": "^1.13.0",
        "@vue/cli-plugin-e2e-cypress": "^3.12.1",
        "@vue/cli-plugin-eslint": "^3.12.1",
        "@vue/cli-plugin-typescript": "^3.12.1",
        "@vue/cli-plugin-unit-mocha": "^3.12.1",
        "@vue/cli-service": "^3.12.1",
        "@vue/eslint-config-prettier": "^4.0.1",
        "@vue/eslint-config-typescript": "^4.0.0",
        "@vue/test-utils": "1.0.0-beta.29",
        "chai": "^4.1.2",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^4.3.0",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.18.3",
        "eslint-plugin-react-hooks": "^1.7.0",
        "eslint-plugin-vue": "^5.2.3",
        "nodemon": "^1.19.4",
        "prettier": "^1.19.1",
        "pretty-quick": "^1.11.1",
        "sass": "^1.25.0",
        "sass-loader": "^7.3.1",
        "stylus": "^0.54.7",
        "stylus-loader": "^3.0.1",
        "ts-loader": "^6.2.1",
        "typescript": "^3.7.5",
        "vue-cli-plugin-vuetify": "^0.5.0",
        "vue-eslint-parser": "^6.0.5",
        "vue-template-compiler": "^2.6.11",
        "vuetify-loader": "^1.4.3"
    },
    "engines": {
        "node": "10.x"
    }
}
